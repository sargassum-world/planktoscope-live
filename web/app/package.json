{
  "name": "pslive",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "rollup -c",
    "check": "svelte-check",
    "format": "prettier --write ./src/**/*.{js,svelte,html} --no-error-on-unmatched-pattern",
    "lint": "prettier --check --write ./src/**/*.{js,svelte,html} --no-error-on-unmatched-pattern && eslint src/**/*.{js,svelte} --no-error-on-unmatched-pattern",
